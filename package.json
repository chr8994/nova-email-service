{
  "name": "nova-email-service",
  "version": "1.0.0",
  "description": "Background service for processing Nylas webhook notifications from PGMQ queue",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:webhooks": "tsx watch src/webhooks.ts",
    "dev:backfill": "tsx watch src/backfill.ts",
    "dev:threads": "tsx watch src/threads.ts",
    "dev:completion": "tsx watch src/completion.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:webhooks": "node dist/webhooks.js",
    "start:backfill": "node dist/backfill.js",
    "start:threads": "node dist/threads.js",
    "start:completion": "node dist/completion.js",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nylas",
    "email",
    "webhook",
    "pgmq",
    "queue-processor"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "dotenv": "^16.3.1",
    "nylas": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
